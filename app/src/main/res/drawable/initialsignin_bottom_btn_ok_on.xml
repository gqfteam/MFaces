<?xml version="1.0" encoding="utf-8"?>
<shape
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle"
    >
    <!--填充颜色-->
    <solid android:color="@color/blueColor"/>
    <!--边框的颜色-->
    <stroke
        android:color="@color/gray"
        android:width="1dp"
        android:dashGap="0dp"/>

    <!--设置圆角的度数-->
    <corners
        android:radius="5dp"
        />



</shape>